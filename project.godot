; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="NITELog"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Firebase="*res://addons/godot-firebase/firebase/firebase.tscn"
Global="*res://scripts/GlobalUtils.gd"
Notifications="*res://scripts/Notifications.gd"

[display]

window/size/mode=2
window/size/borderless=true

[dotnet]

project/assembly_name="NITELog"

[editor_plugins]

enabled=PackedStringArray("res://addons/SystemTrayEnabler/plugin.cfg", "res://addons/godot-firebase/plugin.cfg", "res://addons/qr_code/plugin.cfg")

[rendering]

textures/canvas_textures/default_texture_filter=2
